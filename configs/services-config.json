{
  "services": {
    "proxy": {
      "name": "proxy",
      "region": "europe-west1",
      "runtime": "python311",
      "memory": "512MB",
      "timeout": "300s",
      "authentication": "public",
      "secrets": ["DISCORD_PUBLIC_KEY", "DISCORD_BOT_TOKEN", "DISCORD_APPLICATION_ID"],
      "env_vars": ["GCP_PROJECT_ID", "ENVIRONMENT", "USER_MANAGER_URL", "AUTH_SERVICE_URL"]
    },
    "user-manager": {
      "name": "user-manager",
      "region": "europe-west1",
      "runtime": "python311",
      "memory": "512MB",
      "timeout": "300s",
      "authentication": "private",
      "secrets": [],
      "env_vars": ["GCP_PROJECT_ID", "ENVIRONMENT", "FIRESTORE_DATABASE", "RATE_LIMITS_JSON"]
    },
    "canvas-service": {
      "name": "canvas-service",
      "region": "europe-west1",
      "runtime": "python311",
      "memory": "512MB",
      "timeout": "540s",
      "authentication": "private",
      "secrets": [],
      "env_vars": ["GCP_PROJECT_ID", "ENVIRONMENT", "FIRESTORE_DATABASE", "GCS_CANVAS_BUCKET", "USER_MANAGER_URL"]
    },
    "auth-service": {
      "name": "discord-auth-service",
      "region": "europe-west1",
      "runtime": "python311",
      "memory": "512MB",
      "timeout": "300s",
      "authentication": "public",
      "secrets": ["DISCORD_CLIENT_ID", "DISCORD_CLIENT_SECRET", "DISCORD_REDIRECT_URI", "WEB_FRONTEND_URL"],
      "env_vars": ["GCP_PROJECT_ID", "ENVIRONMENT", "FIRESTORE_DATABASE"]
    },
    "web-frontend": {
      "name": "web-frontend",
      "region": "europe-west1",
      "runtime": "python311",
      "memory": "256MB",
      "timeout": "120s",
      "authentication": "public",
      "secrets": [],
      "env_vars": ["WEB_FRONTEND_URL", "GATEWAY_URL", "AUTH_SERVICE_URL"]
    },
    "discord-registrar": {
      "name": "discord-utils",
      "region": "europe-west1",
      "runtime": "python311",
      "memory": "512MB",
      "timeout": "300s",
      "authentication": "public",
      "secrets": ["DISCORD_BOT_TOKEN", "DISCORD_APPLICATION_ID"],
      "env_vars": ["GCP_PROJECT_ID", "ENVIRONMENT"]
    },
